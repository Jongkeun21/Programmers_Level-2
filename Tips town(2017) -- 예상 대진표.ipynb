    "def solution(n, a, b) :\n",
    "    answer = 0\n",
    "\n",
    "    while a != b :\n",
    "        if a%2 == 1 :\n",
    "            a += 1\n",
    "        if b%2 == 1 :\n",
    "            b += 1\n",
    "\n",
    "        a = a//2\n",
    "        b = b//2\n",
    "\n",
    "        answer += 1\n",
    "    \n",
    "    return answer"
   ]
  }
