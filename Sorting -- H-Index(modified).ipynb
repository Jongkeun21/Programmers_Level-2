    "def solution(citations) :\n",
    "    answer = 0\n",
    "    citations.sort(reverse=True)\n",
    "    \n",
    "    for i in range(len(citations)) :\n",
    "        if i+1 == citations[i] or i+1 < citations[i] :\n",
    "            answer = i+1\n",
    "            \n",
    "    return answer"
   ]
  }
